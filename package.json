    "jito-ts": "^4.2.0",
    "jsonwebtoken": "^9.0.2",
    "lowdb": "^7.0.1",
    "lucide-react": "^0.344.0",
    "next-themes": "^0.4.4",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^6.22.1",
    "recharts": "^2.15.1",
    "speakeasy": "^2.0.0",
    "tailwind-merge": "^2.6.0",
