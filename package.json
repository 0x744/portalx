  "build": {
    "appId": "com.portalx.app",
    "productName": "PortalX",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "main.js"
    ],
    "mac": {
      "category": "public.app-category.finance"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
