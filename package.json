{
  "name": "portalx",
  "private": true,
  "version": "0.1.0",
  "description": "Launch, Manage, and Dominate Token Development on Solana",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "electron .",
    "dev": "vite",
    "build": "tsc",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "tsx src/tests/run.ts",
    "test:coverage": "vitest run --coverage",
    "docs": "typedoc --out docs src",
    "docs:watch": "typedoc --watch",
    "test:watch": "ts-node-dev --respawn src/tests/runTests.ts"
  },
